<%- include('../template/head') -%>

<body>
  <div>
    <%- include('../template/nav.ejs')-%>

    <main>
      <h1>Table </h1>
      <table id="table" , border="2px solid black">
        <thead>
          <tr>
            <th>Website/Framework</th>
            <th>URL</th>
          </tr>
        </thead>
        <tbody>
          <%table.forEach((entry) => {%>
            <tr>
              <td><%=entry.name%></td>
              <td><%=entry.url%></td>
            </tr>
          <%});%>
        </tbody>
      </table>
    </main>

    <%- include('../template/footer.ejs')-%>
  </div>

</body>
</html>